{
  "name": "Fitness Extension",
  "version": "1.3",
  "description": "An extension that allows you to set an alarm to do a workout and a new webpage pops up with a workout displayed.",
  "permissions": ["alarms", "storage"],

  "background": { 
    "scripts": ["background.js"],
    "persistent": false
  },

  "browser_action": {
    "default_icon": "dumbbell2(16).png",
    "default_title": "Fitness Extension",
    "default_popup": "popup.html"
  },

  "icons": {
    "16": "dumbbell2(16).png",
    "32": "dumbbell2(32).png",
    "48": "dumbbell2(48).png",
    "128": "dumbbell2(128).png"
  },

  "manifest_version": 2
}
